class Invirtiendocadenas {
    public static void main(String[] args) {
        System.out.println("Try programiz.pro");
        String nombre = "Hola Mafe";
        String revertir = "";
        for (int i = nombre.length() - 1; i > -1; i--) {
            revertir += nombre.charAt(i);
        }
        System.out.println(revertir);
    }
}
